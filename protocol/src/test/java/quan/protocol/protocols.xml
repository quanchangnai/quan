<?xml version="1.0" encoding="UTF-8"?>
<protocols package="quan.protocol.role">
    角色类型
    <enum name="RoleType">
        <field name="type1" value="1"/>角色类型1
        <field name="type2" value="2"/>角色类型2
    </enum>

    <bean name="RoleInfo">角色信息
        <field name="roleId" type="long" value="111"/>角色id
        <field name="bo" type="bool" value="true"/>
        <field name="by" type="byte" value="22"/>
        <field name="s" type="short" value="22"/>
        <field name="i" type="int" value="11"/>
        <field name="f" type="float" value="22.33332"/>
        <field name="d" type="double" value="33.332432"/>
        <field name="roleName" type="string" value="zhangsan"/>角色名
        <field name="roleType" type="RoleType" value="type1"/>
        <field name="data" type="bytes"/>
        <field name="list" type="list" value-type="int"/>
        <field name="set" type="set" value-type="int"/>
        <field name="map" type="map" key-type="int" value-type="int"/>
    </bean>

    <bean name="UserInfo">
        <field name="name" type="string"/>
        <field name="level" type="int"/>
        <field name="experience" type="int"/>
        <field name="icon" type="int"/>
        <field name="power" type="int"/>
        <field name="modifyNameCount" type="int"/>
        <field name="eventState" type="string"/>
        <field name="functionState" type="string"/>
        <field name="lucky" type="int"/>
        <field name="currentState" type="int"/>
        <field name="buyPowerCount" type="int"/>
    </bean>

    <protocol id="2222" name="SRoleLogin">角色登录
        <field name="roleId" type="long" value="111"/>角色id
        <field name="roleInfo" type="RoleInfo" optional="true"/>角色信息
        <field name="roleInfoList" type="list" value-type="RoleInfo"/>角色信息
        <field name="roleInfoSet" type="set" value-type="RoleInfo"/>角色信息
        <field name="roleInfoMap" type="map" key-type="long" value-type="RoleInfo"/>角色信息
    </protocol>

</protocols>