<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.stage.Screen?>
<BorderPane maxHeight="${screen.visualBounds.height}" maxWidth="${screen.visualBounds.width}" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600" prefWidth="800"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quan.test.roster.RosterController">
    <fx:define>
        <Screen fx:factory="getPrimary" fx:id="screen"/>
    </fx:define>
    <center>
        <TableView fx:id="tableView" editable="true" prefHeight="507.0" prefWidth="821.0" style="-fx-background-color: transparent;" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn prefWidth="75.0" text="ID"/>
                <TableColumn prefWidth="130.0" text="名字"/>
                <TableColumn prefWidth="75.0" text="电话"/>
                <TableColumn prefWidth="75.0" text="地址"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
            <BorderPane.margin>
                <Insets bottom="-1.0" left="-1.0" right="-1.0" top="-1.0"/>
            </BorderPane.margin>
        </TableView>
    </center>
    <bottom>
        <Pagination fx:id="pagination" pageCount="200" pickOnBounds="false" prefHeight="65.0" prefWidth="821.0" style="-fx-max-page-indicator-count: 20; -fx-page-information-visible:false"
                    BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="10.0"/>
            </padding>
        </Pagination>
    </bottom>
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <Menu mnemonicParsing="false" text="File">
                <MenuItem mnemonicParsing="false" text="Close"/>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
                <MenuItem mnemonicParsing="false" text="Delete"/>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
                <MenuItem mnemonicParsing="false" text="About"/>
            </Menu>
        </MenuBar>
    </top>
</BorderPane>
